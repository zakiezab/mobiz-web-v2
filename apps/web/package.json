    "next": "15.1.5",
    "next-sanity": "^11.6.3",
    "next-themes": "^0.4.6",
    "package-json": "^10.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "resend": "^4.0.0",
    "sanity": "^4.12.0",
